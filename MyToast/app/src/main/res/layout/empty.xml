<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <ImageView
        android:id="@+id/empty_iv_1"
        android:layout_width="139.4dp"
        android:layout_height="166dp"
        android:layout_marginTop="181.1dp"
        android:background="@color/white"
        android:src="@drawable/toest_logo_2"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/empty_tv_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50.9dp"
        android:text="오답노트가 텅 비었어요!"
        android:textColor="@color/textGray"
        android:textSize="20dp"
        android:textStyle="normal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/empty_iv_1" />

    <TextView
        android:id="@+id/empty_tv_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LC음원을 추가하여 공부해보세요."
        android:textColor="@color/textGray"
        android:textSize="20dp"
        android:textStyle="normal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/empty_tv_1" />

</androidx.constraintlayout.widget.ConstraintLayout>